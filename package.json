{
  "name": "@deepgram/prerecorded-node-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "node --no-deprecation server.js",
    "start:frontend": "cd frontend && npm run dev -- --port 5173 --no-open --logLevel error",
    "dev": "nodemon --no-deprecation server.js"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.3",
    "multer": "^2.0.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}