{
  "name": "@deepgram/transcription-html",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/transcription-html",
  "projectType": "application",
  "tags": [
    "frontend",
    "vite",
    "transcription",
    "html"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dev",
        "cwd": "apps/transcription-html"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": ["@deepgram/styles"]
        }
      ]
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "apps/transcription-html"
      },
      "outputs": [
        "{projectRoot}/dist"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm preview",
        "cwd": "apps/transcription-html"
      },
      "dependsOn": [
        "build"
      ]
    }
  }
}

